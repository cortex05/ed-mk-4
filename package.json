{
  "name": "ed-mk-4",
  "version": "1.0.0",
  "description": "Attempt with typescript",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node backend/src/server.ts",
    "start:prod": "ts-node backend/src/server.ts",
    "build": "tsc -p backend/ && tsc -p frontend/",
    "server": "nodemon backend/src/server.ts",
    "client": "npm dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"cd frontend && npm run dev\""
  },
