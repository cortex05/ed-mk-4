  "dependencies": {
    "axios": "^1.12.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.2",
    "concurrently": "^9.2.1",
